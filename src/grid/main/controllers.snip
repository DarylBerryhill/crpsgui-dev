    <h1 class="panyeTitle">Controllers and Chargers</h1>
    <!--  CONTROLLER #1  -->
    <div id="controller1" class="hide">
        <div class="h1Float">
            Controller
            <span class="bold">#1</span> addr:
            <span class="bold">0x18</span>
            <span> Status: </span>
            <span id="c1Status" class="bold"></span>
        </div>
        <div id="c1Battery"
             class="batteryPresent"
             style="float: left; font-size: 2.2em; padding-top: 0.5em;"></div>
        <div style="clear: left;">
            <button id="CTRL1Green"
                    address="0x18"
                    regOnData="+0x80"
                    regOffData="-0x80"
                    btnColor="green">GREEN led</button>
            <button id="CTRL1Red"
                    address="0x18"
                    regOnData="+0x40"
                    regOffData="-0x40"
                    btnColor="red">Red led</button>
            <button id="CTRL1MChargeEn"
                    address="0x18"
                    regOnData="+0x08"
                    regOffData="-0x08"
                    btnColor="indigo">Master Charge Enable</button>
        </div>
        <div class="center-column-header">
            <span> </span>
            <span>Threshold</span>
            <span style="left: 13em;">Status</span>
        </div>
        <div class="chargerA">
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Charger <b>A</b>
            <span id="c1AId"     class="bold"> </span>
            <span id="c1AVth"    class="space1 "> Voltage </span>
            <span id="c1AIth"    class="space2"> Current </span>
        </div>
        <div class="center-column-header">
            <span id="c1a_pg"  class="bitfields" >PG</span>
            <span id="c1a_otp" class="bitfields" >OTP</span>
            <span id="c1a_otw" class="bitfields" >OTW</span>
            <span id="c1a_status"
                  style="display: inline-block; margin-right: 0.7em;"
                  class="cccv_status"></span>
            <span id="c1avolt"
                  style="display: inline-block; margin-right: 0.7em;"
                  class="bold"></span>
            <span id="c1acurrent" class="bold"></span>
            <span id="c1AStatus" class="space3 bold"> </span>
        </div>
        <div class="chargerB">
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Charger <b>B</b>
            <span id="c1BId"     class="bold"> </span>
            <span id="c1BVth"    class="space1"> Voltage </span>
            <span id="c1BIth"    class="space2"> Current </span>
        </div>
        <div class="center-column-header">
            <span id="c1b_pg"  class="bitfields" >PG</span>
            <span id="c1b_otp" class="bitfields" >OTP</span>
            <span id="c1b_otw" class="bitfields" >OTW</span>
