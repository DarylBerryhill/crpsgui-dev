<div>
    <style>
        #controller1,
        #controller2,
        #controller3,
        #controller4
        {
            float: left;
            width: 65em;
            border: 0.2em solid  #e0d5d5;
            border-radius: 1em;
            box-shadow: 0em 0.1em 0.7em rgba(66,66,66,.64);
            margin:  0em 2.0em 2.0em 1.2em;
            padding: 0.7em 0em 1.0em 0.5em;
            background-color: lightslategrey;
            overflow: hidden;
        }

        .h1Float
        {
            float: left;
            font-size: 2.2em;
            margin: 0em 1.0em 0.5em 0.7em;
            border: 0.1em solid #e0d5d5;
            border-radius: 1.0em;
            padding: 0.2em 0.6em 0.2em 0.4em;
            background-color:cadetblue
        }
        .batteryPresent
        {
            font-size: 2.0em;
            color:bisque;
        }

        .chargerA
        {
            font-size: 2.2em;
        }
        .chargerB
        {
            font-size: 2.2em;
            margin-top: 1em;
        }

        .space1
        {
            display: inline;
            position: relative;
            left: 21px;
        }

        .space2
        {
            display: inline;
            position: relative;
            left: 25px;
        }

        .space3
        {
            display: inline;
            position: relative;
            left: 14px;
        }

        .center-column-header
        {
            display: inline-block;
            font-weight: bolder;
            font-size: 2.2em;
        }
 
        .orange2
        {
         background-color: coral;
         color: white;
        }
        .yellow2
        {
         background-color: yellow;
         color: black;
        }
        .lightgreen
        {
            background-color: lightgreen;
        }
        .lightcoral
        {
            background-color: lightcoral;
        }
        .cccv_status
        {
          display: inline-block;
          padding: 0.3em;
          font-size: 0.7em;
          border: 0.1em black solid;
          border-radius: 50%;
          font-weight: bolder;
        }

        .bitfields
        {
            display: inline-block;
            margin: 0;
            padding: 0.2em;
            border: 0.1em black solid;
            border-radius: 50%;
            font-size: 0.7em;
